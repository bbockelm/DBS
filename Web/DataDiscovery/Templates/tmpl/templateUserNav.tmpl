<!-- templateUserNav.tmpl -->
<!-- begin outer most table -->
<table class="lfn">
<tr valign="top">
<td valign="top">

<form action="getData" method="get">
###<form action="javascript:ResetAllResults();MakeUserNavBar();showResMenu('results');ajaxGetUserData()" method="get">

<input type="hidden" name="ajax" value="0" />
<input type="hidden" name="userMode" value="$userMode" id="kw_userModeSelector" />
<!-- begin menu table -->
<table class="small" cellspacing="5">

#if $userMode=='user'
<input type="hidden" name="dbsInst" value="$dbsInst" id="kw_dbsInstSelector" />
#else
<tr>
<td align="right">
<div id="dbsInstDef">
<b>DBS instances</b>
</div>
</td>
#*
<td>
<div class="yellow_box" onMouseOver="KeywordHelp('kw_dbs_tag','<p>List of known DBS instances.</p>')" onMouseOut="ClearTag('kw_dbs_tag')">?</div><span id="kw_dbs_tag"></span></td>
*#
<td>
<select name="dbsInst" id="kw_dbsInstSelector" onchange="javascript:ajaxGetKWFields()">
#for dbs in $dbsList
<option value="$dbs">$dbs</option>
#end for
</select>
</td>
</tr>
#end if


<tr>
<td align="right">
<div id="groupDef">
<b>Physics groups</b>
</div>
</td>
#*
<td><div class="yellow_box" onMouseOver="KeywordHelp('kw_groups_tag','<p>Here you select the physics group.</p> <p><b>Example:</b> Higgs, top group.</p>')" onMouseOut="ClearTag('kw_groups_tag')">?</div><span id="kw_groups_tag"></span></td>
*#
<td>

<div id="kw_group_holder">
<select name="group" id="kw_group">
<option value="Any">Any</option>
#for group in $groupList
<option value="$group">$group</option>
#end for
</select>
</div>

</td>
</tr>


<tr>
<td valign="top" align="right">
<div id="tierDef">
<b>Data tier</b>
</div>
</td>
#*
<td><div class="yellow_box" onMouseOver="KeywordHelp('kw_tier_tag','<p>This menu shows all known in DBS data types.</p> <p><b>Example:</b> DIGI, RECO.</p>')" onMouseOut="ClearTag('kw_tier_tag')">?</div><span id="kw_tier_tag"></span></td>
*#
<td>

<div id="kw_tier_holder">
<select name="tier" id="kw_tier">
<option value="Any">Any</option>
#for tier in $dataTypes
<option value="$tier">$tier</option>
#end for
</select>
</div>

</td>
</tr>

<tr>
<td valign="top" align="right">
<div id="softRelDef">
<b>Software releases</b>
</div>
</td>
#*
<td><div class="yellow_box" onMouseOver="KeywordHelp('kw_rel_tag','<p>All known software releases.</p> <p><b>Example:</b> CMSSW_1_1_1</p>')" onMouseOut="ClearTag('kw_rel_tag')">?</div><span id="kw_rel_tag"></span></td>
*#
<td>

<div id="kw_release_holder">
<select name="app" id="kw_release" onchange="javascript:ajaxUpdatePrimaryDatasets();">
<option value="Any" selected="selected">Any</option>
#for rel in $softReleases
<option value="$rel">$rel</option>
#end for
</select>
</div>

</td>
</tr>

<tr>
<td valign="top" align="right">
<div id="primDef">
<b>Primary dataset/<br />MC generators</b>
</div>
</td>
#*
<td><div class="yellow_box" onMouseOver="KeywordHelp('kw_prim_tag','<p>This is a list of primary datasets known in DBS. In a case of real data it is trigger lines, in a case of MC it is MC generators.</p> <p><b>Example:</b> mc-onsel-120_PU_Zee </p>')" onMouseOut="ClearTag('kw_prim_tag')">?</div><span id="kw_prim_tag"></span></td>
*#
<td>

<div id="kw_prim_holder">
<select name="primD" id="kw_prim">
<option value="Any" selected="selected">Any</option>
#for prim in $primDatasets
<option value="$prim">$prim</option>
#end for
</select>
</div>

</td>
</tr>

#if $userMode=='user'
<input type="hidden" name="site" value="Any" id="kw_site" />
#else
<tr>
<td valign="top" align="right">
<div id="siteDef">
<b>Sites</b>
</div>
</td>
#*
<td><div class="yellow_box" onMouseOver="KeywordHelp('kw_site_tag','<p>List of know CMS sites</p> <p><b>Example: cmssrm.fnal.gov</b></p>')" onMouseOut="ClearTag('kw_site_tag')">?</div><span id="kw_site_tag"></span></td>
*#
<td>

<div id="kw_site_holder">
<select name="site" id="kw_site">
<option selected="selected" value="All">Any</option>
#for site in $siteList
<option>$site</option>
#end for
</select>
</div>

</td>
</tr>
#end if

<tr>
<td>
</td>
###<td></td>
<td>
###<input type="submit" value="Find" onclick="javascript:checkNavSelection()" />
<input type="submit" value="Find" />
</td>
</tr>
</table>

<!-- end of menu table -->
</form>
</td>
</tr>
</table>
<!-- end of outer most table -->

<script type="text/javascript">
var tooltip;
tooltip="<p>List of known DBS instances.</p>";
MakeTooltip("t_dbsInst","dbsInstDef",tooltip);
tooltip="<p>Here you select the physics group.</p> <p><b>Example:</b> Higgs, top group.</p>";
MakeTooltip("t_group","groupDef",tooltip);
tooltip="<p>This menu shows all known in DBS data types.</p> <p><b>Example:</b> DIGI, RECO.</p>";
MakeTooltip("t_tier","tierDef",tooltip);
tooltip="<p>All known software releases.</p> <p><b>Example:</b> CMSSW_1_1_1</p>";
MakeTooltip("t_softRel","softRelDef",tooltip);
tooltip="<p>This is a list of primary datasets known in DBS. In a case of real data it is trigger lines, in a case of MC it is MC generators.</p> <p><b>Example:</b> mc-onsel-120_PU_Zee </p>";
MakeTooltip("t_primDef","primDef",tooltip);
tooltip="<p>List of know CMS sites</p> <p><b>Example: cmssrm.fnal.gov</b></p>";
MakeTooltip("t_site","siteDef",tooltip);
</script>

<!-- end of templateUserNav.tmpl -->
